<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.kyrgyzpedigree">
    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:usesCleartextTraffic="true"
        android:allowBackup="true"
        android:icon="@drawable/logo_sanjyra"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Activity_Maps"/>
        <activity android:name=".LoginAdmin"></activity>
        <activity android:name=".EnterActivity" />
        <activity android:name=".infoActivity" />
        <activity android:name=".delete_note" />
        <activity android:name=".resultActivity" />
        <activity android:name=".showSanjyra" />
        <activity android:name=".addDataActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <uses-library
            android:name="org.apache.http.legacy"
            android:required="false" />

	<meta-data
    	android:name="com.google.android.gms.version"
    	android:value="@integer/google_play_services_version" />
	
	<meta-data
  	android:name="com.google.android.geo.API_KEY"
  	android:value="@string/google_maps_key" />
    </application>

</manifest>
